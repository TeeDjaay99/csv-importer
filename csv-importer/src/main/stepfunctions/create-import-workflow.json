{
  "Comment": "Mitt workflow f√∂r att notifiera via SNS efter import",
  "StartAt": "ValidateInput",
  "States": {
    "ValidateInput": {
      "Type": "Pass",
      "Result": "Input validated",
      "Next": "PublishToSNS"
    },
    "PublishToSNS": {
      "Type": "Task",
      "Resource": "arn:aws:states:::aws-sdk:sns:publish",
      "Parameters": {
        "TopicArn": "arn:aws:sns:eu-north-1:746669216309:csv-importer-events",
        "Subject": "Import Notification",
        "Message.$": "States.JsonToString($)"
      },
      "End": true
    }
  }
}
